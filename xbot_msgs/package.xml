<package>
  <name>xbot_msgs</name>
  <version>0.6.1</version>
  <description>
    <p>
      Xbot message and service types: custom messages and services for Xbot packages.
    </p> 
  </description>
  <author email="yowlings@gmail.com">Roc Wang</author>
  <maintainer email="yowlings@gmail.com">Roc Wang</maintainer>
  <license>BSD</license>
  <url type="website">http://ros.org/wiki/xbot_msgs</url>
  <url type="repository">https://github.com/yujinrobot/xbot_msgs</url>
  <url type="bugtracker">https://github.com/yujinrobot/xbot_msgs/issues</url>
  
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>std_msgs</build_depend>
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>message_generation</build_depend>

  <run_depend>std_msgs</run_depend>
  <run_depend>actionlib_msgs</run_depend>
  <run_depend>message_runtime</run_depend>
</package>
